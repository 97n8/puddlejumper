{
  "version": 2,
  "builds": [
    { "src": "apps/puddlejumper/src/api/server.ts", "use": "@vercel/node" },
    { "src": "apps/puddlejumper/public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "apps/puddlejumper/src/api/server.ts" },
    { "src": "/(.*)", "dest": "apps/puddlejumper/public/$1" }
  ],
  "functions": {
    "apps/puddlejumper/src/api/server.ts": { "maxDuration": 60 }
  }
}
